{
  "name": "setbased/php-audit",
  "description": "A tool for creating audit tables and triggers for logging data changes in a MySQL database.",
  "keywords": [
    "MySQL",
    "audit",
    "triggers"
  ],
  "type": "tool",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "*",
    "setbased/phing-extensions": "2.*"
  },
  "require": {
    "monolog/monolog": "1.*",
    "php": ">=5.4.0",
    "setbased/php-affirm": "*",
    "setbased/php-stratum": "*"
  },
  "autoload": {
    "psr-4": {
      "SetBased\\Audit\\": "src/"
    }
  },
  "config": {
    "bin-dir": "bin"
  }
}
